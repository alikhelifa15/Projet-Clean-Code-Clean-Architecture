{
    "compilerOptions": {
      "target": "ES6", 
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "module": "commonjs",
      "typeRoots": ["@types", "node_modules/@types"],
      "moduleResolution": "node",
      "resolveJsonModule": true, 
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["node"]
    },
    "include": [
      "models/**/*.ts",
    ]
  }
  